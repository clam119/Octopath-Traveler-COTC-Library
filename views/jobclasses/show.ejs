<h2 class="page-header"><%= jobclass.name %></h2>
<div class="btn-row">
  <a class="btn btn-primary" href="/jobclasses/<%= jobclass.id %>/edit">Edit</a>
  <%- include('../partials/deleteForm',
              { url: `/jobclasses/${jobclass.id}` }) %>
</div>
<% if (jobclasses.length > 0) { %>
  <h2 class="page-header">Characters By Jobclass</h2>
  <%- include('../partials/characterGrid', { characters: characters }) %>
<% } %>